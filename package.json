  "author": "james@reciperadar.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openculinary/i18next-gettext-loader/issues"
  },
  "homepage": "https://github.com/openculinary/i18next-gettext-loader",
  "dependencies": {
    "i18next-conv": "^15.1.2"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "json-loader": "^0.5.7",
    "memfs": "^4.49.0",
    "webpack": "^5.102.1"
  }
